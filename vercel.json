{
  "version": 2,
  "builds": [
    {
      "src": "app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "routes/**",
          "utils/**",
          "views/**",
          "models/**",
          "oke/**",
          "config.mjs",
          "temp/**"
        ]
      }
    },
    {
      "src": "oke/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/oke/(.*)",
      "dest": "/oke/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}