{
  "builds": [
    {
      "src": "app.mjs",
      "config": {
        "includeFiles": [
          "routes/page.mjs",
          "routes/signed-in.mjs",
          "utils/db.mjs",
          "utils/password.mjs",
          "views/**",
          "models/user.mjs",
          "public/style.css",
          "config.mjs"
        ],
        "use": "@vercel/node"
      }
    }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.mjs"
      }
    ]
  }