{
  "version": 2,
  "builds": [
    {
      "src": "app.mjs",
      "use": "@vercel/node",
      "includeFiles": [
        "utils/**/*", 
        "models/**/", 
        "config.mjs", 
        "routes/**/*",
        "temp/**/*",
        "public/**/*"
        ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.mjs"
    }
  ]
}
