{
  "version": 2,
  "builds": [
    {
      "src": "app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "routes/**",
          "utils/**",
          "views/**",
          "models/**",
          "public/**",
          "config.mjs",
        ]
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.mjs"
    }
  ]
}