{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "views/**/*", "use": "@vercel/static" },
    { "src": "oke/**/*", "use": "@vercel/static" },
    { "src": "models/**/*", "use": "@vercel/static" },
    { "src": "routes/**/*", "use": "@vercel/static" },
    { "src": "utils/**/*", "use": "@vercel/static" },
    { "src": "config.mjs", "use": "@vercel/static" },
    { "src": "temp/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
